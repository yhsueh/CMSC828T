<launch>
	<arg name="marker_size" default="4.4" />
	<arg name="max_new_marker_error" default="0.08" />
	<arg name="max_track_error" default="0.2" />

	<arg name="cam_image_topic" default="/webcam/image_raw" />
	<arg name="cam_info_topic" default="/webcam/camera_info" />
	<arg name="output_frame" default="/webcam_frame" />
	<arg name="bundle_files" default="$(find CMSC828T)/bundles/table_8_9_10.xml" />

	<node name="artag_node" pkg="ar_track_alvar" type="findMarkerBundlesNoKinect" respawn="false" output="screen" args="$(arg marker_size) $(arg max_new_marker_error) $(arg max_track_error) $(arg cam_image_topic) $(arg cam_info_topic) $(arg output_frame) $(arg bundle_files)" />

	<!--node name="artag_node" pkg="ar_track_alvar" type="findMarkerBundlesNoKinect" respawn="false" output="screen">
		<param name="marker_size" type="double" value="$(arg marker_size)" />
		<param name="max_new_marker_error" type="double" value="$(arg max_new_marker_error)" />
		<param name="max_track_error" type="double" value="$(arg max_track_error)" />
		<param name="output_frame" type="string" value="$(arg output_frame)" />
		<param name="bundle_files" type="string" value="$(arg bundle_files)" />

		<remap from="camera_image" to="$(arg cam_image_topic)" />
		<remap from="camera_info" to="$(arg cam_info_topic)" />
	</node-->

</launch>
